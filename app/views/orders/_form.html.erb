<%= simple_form_for(@order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :offer_id %>
    <%= f.input :state %>
    <%= f.input :num_weeks %>
    <%= f.input :total_price %>
    <%= f.input :offer_code %>
    <%= f.input :start_date %>
    <%= f.input :expire_date %>
    <%= f.input :num_seats %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
