<div class="well">
	<legend>
		Application Settings
	</legend>

	<%= simple_form_for(@admin_app_setting) do |f| %>
	  <%= f.error_notification %>

	  <div class="form-inputs">
	    <%= f.input :value, label: 'Increase school offer price by:' %>
	  </div>

	  <div class="form-actions">
	    <%= f.button :submit %>
	  </div>
	<% end %>
</div>